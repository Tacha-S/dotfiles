[user]
	name = Tatsuroh Sakaguchi
	email = tacchan.mello.ioiq@gmail.com
[push]
	default = simple
[alias]
  alias = !git config --get-regexp '^alias\\.' | sed 's/alias\\.\\([^ ]*\\) \\(.*\\)/\\1\\\t => \\2/' | sort
  cho = checkout
  cm = commit -F-
  graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  pr = pull-request
  see = browse
  refresh= !git fetch origin && git remote prune origin
  unstage = reset -q HEAD --
  chopr = "!f(){ git fetch origin pull/$1/head:pull/$1 && git checkout pull/$1;};f"
  clean-br = "!f () { git checkout $1; git branch --merged|egrep -v '\\*|develop|master'|xargs git branch -d; };f"

[core]
	excludesfile = /home/tacha/.gitignore_global
